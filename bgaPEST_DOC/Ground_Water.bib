@article{
   author = {Narasimhan, T. N.},
   title = {Laplace equation and Faraday's lines of force},
   journal = {Water Resources Research},
   volume = {44},
   number = {9},
   abstract = {Boundary-value problems involve two dependent variables: a potential function and a stream function. They can be approached in two mutually independent ways. The first, introduced by Laplace, involves spatial gradients at a point. Inspired by Faraday, Maxwell introduced the other, visualizing the flow domain as a collection of flow tubes and isopotential surfaces. Boundary-value problems intrinsically entail coupled treatment (or, equivalently, optimization) of potential and stream functions. Historically, potential theory avoided the cumbersome optimization task through ingenious techniques such as conformal mapping and Green's functions. Laplace's point-based approach and Maxwell's global approach each provides its own unique insights into boundary-value problems. Commonly, Laplace's equation is solved either algebraically or with approximate numerical methods. Maxwell's geometry-based approach opens up novel possibilities of direct optimization, providing an independent logical basis for numerical models, rather than treating them as approximate solvers of the differential equation. Whereas points, gradients, and Darcy's law are central to posing problems on the basis of Laplace's approach, flow tubes, potential differences, and the mathematical form of Ohm's law are central to posing them in natural coordinates oriented along flow paths. Besides being of philosophical interest, optimization algorithms can provide advantages that complement the power of classical numerical models. In the spirit of Maxwell, who eloquently spoke for a balance between abstract mathematical symbolism and observable attributes of concrete objects, this paper is an examination of the central ideas of the two approaches, and a reflection on how Maxwell's integral visualization may be practically put to use in a world of digital computers.},
   year = {2008}
   doi = {doi}

}

@phdthesis{Nasser1994,
   author = {Nasser, Khalil},
   title = {Stochastic modeling of the geometry and hydraulics of fractured rocks at the {C}olorado {S}chool of {M}ines experimental mine, {I}daho {S}prings, {C}olorado},
   university = {Colorado School of Mines},
   note = {Includes one computer disk
English},
   type = {Doctoral},
   abstract = {In 1979, a new geotechnical test facility was constructed within the Colorado School of Mines (CSM) Experimental Mine, which is located in the fractured crystalline Precambrian rocks of the Front Range of the Rocky Mountains about 60 kilometers west of Denver, Colorado. As part of this geotechnical facility, the CSM-OCRD Room was excavated and more than 870 individual fractures were mapped from eleven parallel mining faces exposed during excavation. From these observations, this study evaluated the three-dimensional (3D) geometry of the fracture network at the CSM-OCRD Room using statistics of spherical data. These statistics were then used to generate 3D stochastic computer models simulating the fracture network geometry at the CSM-OCRD Room. These 3D stochastic models were validated using the fractal box counting method. Subsequently, the hydraulic behavior of the generated 3D network was evaluated using finite element fracture flow modeling techniques. The statistical evaluation of fracture geometry data suggested that the fracture network can be described in terms of a multimodal spherical distribution consisting of four modes. Two of the modes are symmetrical around their mean direction axis and were modeled using the Fisher distribution. The other two modes lack such symmetry and were modeled using the Kent distribution. These spherical models were used by the FraeMan software, developed by Dershowitz and made available to this project by Golder Associates, Inc., to generate computerized 3D stochastic fracture realizations that have the same statistical properties as the field data. The generated 3D stochastic realizations were then validated using the fractal box counting method. The two-dimensional (2D) fracture trace maps corresponding to the eleven mining faces at the CSM-OCRD Room were analyzed to evaluate whether the 2D trace maps possess fractal behavior, and if so, numerically estimate the fractal dimension of these trace maps. Similarly, the fractal behavior of the 2D stochastic trace maps (generated by slicing the 3D stochastic model) were also evaluated. The results of the fractal study indicate that the scales over which the two series of trace maps exhibit fractal behavior are similar. The fractal dimensions of the trace maps corresponding to the eleven mining faces are tightly clustered, even though the fracture networks appear quite different. They generally range between 1.27 and 1.50, and the average fractal dimension is 1.37. The fractal dimensions of the stochastic trace maps average 1.55, slightly higher than the maximum fractal dimension computed for the 2D field trace maps. However, the estimates are believed to be still in reasonable agreement considering the accuracy of the methods used to calculate the fractal dimension. Accordingly, the stochastic generation of 3D fracture networks by FraeMan utilizing the statistical definitions appears validated. The stochastic network representations were found to accurately model the field conditions and fracture network geometry exhibited by the fractured rock masses found in the area of the CSM-OCRD Room. The global 3D permeability properties of the stochastic fracture networks were computed to determine whether the fracture system behaves as an anisotropic homogeneous medium with a symmetric permeability tensor, and to evaluate the effect of the geometric model statistical parameters on that behavior. Finite element flow analysis implemented in the computer program MAFIC, also made available to this project by Golder Associates, Inc., were used to perform these evaluations. Permeability tensors were calculated for the 3D fracture network simulating the geometry of fractures at the CSM-OCRD Room. At small scales, the permeability tensor of the fracture system is not an ellipsoid, but is indeed very irregular, and the fracture system does not behave as an homogeneous anisotropic medium. As the scale of analysis increased, the shape of the permeability tensor approached an ellipsoid. The results indicate that at scales corresponding to 2.5 meters and higher, the permeability tensor plotted as an ellipsoid. Therefore, at a scale equal to or larger than 2.5 meters, the fracture network behaves as a homogeneous anisotropic medium with a symmetric permeability tensor, and may be modeled as an equivalent porous medium. A sensitivity analysis was performed to evaluate the effect of the concentration parameter k, which describes the dispersion of the fracture orientations around their mean axis. This analysis indicated that the shape of the permeability tensor is sensitive to the values of k, when these values are small, and therefore, higher confidence in the values of k needs to obtained in those cases. Less precision is needed for higher values of k. Similarly, the effect of the ovalness parameter beta , which measures the degree of asymmetry of fracture orientations around the mean axis, was evaluated. It was found that the shape of the permeability tensor is sensitive to the ovalness parameter at higher values of beta , where the fracture distributions are highly oval. Therefore, higher confidence in the value of beta is required when such distributions are being modeled. The effect of fracture intensity on the hydraulic behavior of the fracture system was also evaluated. It was found that as the fracture intensity increases, the shape of the permeability tensor approached an ellipsoid, and the fracture network increasingly behaved as an equivalent porous medium.},
   keywords = {Clear Creek County Colorado; Colorado; computer programs;
excavations; experimental studies; fractals; fractured materials;
fractures; Front Range; geometry; ground water; hydrodynamics;
Idaho Springs Colorado; mines; mining; models; movement;
permeability; statistical analysis; stochastic processes;
three-dimensional models; United States
21, Hydrogeology},
   year = {1994}
}

