@phdthesis{Diprime1988,
   author = {Diprime, Leonard Joseph, Jr.},
   title = {A field study of flow through fractured shale and its comparison to two mathematical models},
   university = {Emory University},
   note = {References: 28; illus. incl. 5 plates
English},
   type = {Map},
   abstract = {It is now widely recognized that fracture networks in rocks can contribute significantly to the transport of contaminants in ground-water systems. The fluid flux through a fracture system is often several orders of magnitude greater than the flux through the matrix, even though the total porosity of the matrix may be much greater than that of the fractures. The volume of interconnected void space actually contributing to flow (the effective porosity) in the fracture network is usually substantially higher than that of the matrix. For these reasons it is important to have the ability to predict flow in fractured systems, to know the scale at which fractures must be studied in the field, and to verify the accuracy of current transport prediction methods. A field study of flow through the highly fractured, Mississippian aged, Floyd Shale of Floyd County, Georgia, was conducted by running a series of NaCl tracer tests. The fracture system was artificially saturated and the tracer injected 40-feet updip from the outcrop. Samples from each discharge point on the outcrop were used to construct tracer discharge hydrographs, and from these the average linear velocity through the fractured shale was determined. From the outcrop 826 fracture measurements of strike, dip, aperture, and density distribution were used to determine major fracture orientations and to apply to D. T. Snows (1968) equivalent continuum model of a cubic fracture system. An average linear velocity for the fracture system was calculated from Snows model using the above fracture data. Also, the aquifer properties determined from the tracer data were applied to the Prickett and Lonnquist Aquifer Simulation Model (1971), an equivalent porous media model, to simulate the fractured flow system observed in the field. An average linear velocity from the Prickett and Lonnquist (1971) model was also determined. The average linear velocities from the above equivalent continuum models were then compared to the velocity measured in the field to: a) determine how closely such models can reproduce the field results, and b) determine what data is most useful in modeling a fractured flow system.},
   keywords = {aquifers; Carboniferous; clastic rocks; data processing; digital
simulation; field studies; Floyd County Georgia; Floyd Shale;
fractured materials; Georgia; ground water; hydrogeology;
hydrographs; Mississippian; movement; northwestern Georgia;
numerical models; Paleozoic; sedimentary rocks; shale; surveys;
tracers; United States; Upper Mississippian; water quality
21, Hydrogeology},
   year = {1988}
}

